{
    "@context": [
      "https://www.w3.org/2019/wot/td/v1",
      {
        "pwpref": "link zu unserem Server",
        "mqv": "http://www.example.org/mqtt-binding#"
      }
    ],
    "id": "will-be-set-by-wot-client-library",
    "title": "MyDispenserThing",
    "@type": "pwpref:Dispenser",
    "securityDefinitions": {
      "basic_sc": {
        "scheme": "basic",
        "in": "header"
      }
    },
    "modes": [
      "MODE_OFFLINE",
      "MODE_AP_ONLY",
      "MODE_HUB_LOCAL",
      "MODE_HUB_INTERNET"
    ],
    "security": [
      "basic_sc"
    ],
    "properties": {
      "status": {
        "@type": "pwpref:ModeState",
        "type": "boolean",
        "observable": true,
        "forms": []
      },
      "level": {
        "@type": "pwpref:LevelState",
        "type": "boolean",
        "observable": true,
        "forms": []
      }
    },
    "actions": {
      "open": {
        "@type": "pwpref:OpenCommand",
        "input": {
          "type": "float"
        },
        "forms": []
      }
    },
    "events": {
      "emptyingredient": {
        "data": {
          "type": "boolean"
        },
        "forms": []
      }
    }
  }
  